<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8">
		<title></title>
	</head>

	<body>
		<script type="text/javascript">
			var a = [11, 25, 74, 99, 8, 666];

			function bubble(a) {
				for(var i = 0; i < a.length - 1; i++) {
					for(var j = 0; j < a.length - 1 - i; j++) {
						if(a[j] > a[j + 1]) {
							var tmp = a[j];
							a[j] = a[j + 1]
							a[j + 1] = tmp
						}
					}
				}
			}
			bubble(a);
			console.log(a);
			
		</script>
	</body>

</html>
